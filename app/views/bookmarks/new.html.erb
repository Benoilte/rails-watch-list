<p>Find me in app/views/bookmarks/new.html.erb</p>

<!-- app/views/reviews/new.html.erb -->

<h1>Add movie</h1>

<%= simple_form_for [@list, @bookmark] do |f| %>
  <%= f.input :comment %>
  <%= f.association :movie %>
  <%= f.submit "Submit review", class: "btn btn-primary" %>
<% end %>

<%= link_to '<i class="fa-solid fa-arrow-rotate-left"></i> Back to list'.html_safe, list_path(@list), class: 'btn btn-primary mt-4' %>
