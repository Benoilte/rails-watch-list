<div class="card-category-lists">
  My list
</div>

<div class="cards">
  <% @lists.each do |list| %>
    <%= link_to(list_path(list), class: "text-decoration-none") do %>
      <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(#)">
        <p><%= list.name %></p>
      </div>
    <% end %>
  <% end %>
</div>
