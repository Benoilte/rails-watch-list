<h1>Lists#index</h1>
<p>Find me in app/views/lists/index.html.erb</p>

<h1>Lists</h1>

<ul class="list-unstyled">
  <% @lists.each do |list| %>
    <li><%= link_to list.name, list_path(list), class: 'text-decoration-none' %></li>
  <% end %>
</ul>

<%= link_to '<i class="fa-solid fa-circle-plus"></i> Add a new list'.html_safe, new_list_path, class: 'btn btn-primary' %>
